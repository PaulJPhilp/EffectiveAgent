{
  "name": "@effective-agents/cli",
  "version": "1.0.0",
  "description": "Official command-line interface for the Effective Agents framework",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "ea-cli": "dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/effective-agents/ea-cli.git"
  },
  "license": "MIT",
  "author": "Effective Agents Team",
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  },
  "scripts": {
    "build": "tsc",
    "dev": "NODE_OPTIONS='--experimental-specifier-resolution=node --experimental-json-modules' bun run src/index.ts",
    "test": "vitest",
    "format": "biome format --write .",
    "lint": "biome check ."
  },
  "dependencies": {
    "effect": "3.16.4",
    "@effect/cli": "0.63.8",
    "@effect/platform": "0.84.8",
    "@effect/platform-node": "0.85.8",
    "@effect/printer": "0.44.4",
    "@effect/printer-ansi": "0.44.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.15.30",
    "typescript": "^5.8.3",
    "vitest": "^3.2.2"
  },
  "keywords": [
    "ai",
    "agents",
    "cli",
    "effective-agents",
    "framework",
    "automation"
  ],
  "bugs": {
    "url": "https://github.com/effective-agents/ea-cli/issues"
  },
  "homepage": "https://github.com/effective-agents/ea-cli#readme",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}