{
    "name": "test-workbench",
    "description": "Test workbench for toolbox service tests",
    "toolboxes": [
        {
            "name": "math-tools",
            "description": "Mathematical tools",
            "tools": [
                {
                    "name": "calculator",
                    "description": "Basic calculator tool",
                    "input_schema": {
                        "type": "object",
                        "properties": {
                            "expression": {
                                "type": "string"
                            }
                        }
                    },
                    "output_schema": {
                        "type": "object",
                        "properties": {
                            "result": {
                                "type": "number"
                            }
                        }
                    }
                }
            ]
        },
        {
            "name": "utility-tools",
            "description": "Utility tools",
            "tools": [
                {
                    "name": "date-formatter",
                    "description": "Date formatting tool",
                    "input_schema": {
                        "type": "object",
                        "properties": {
                            "date": {
                                "type": "string"
                            },
                            "format": {
                                "type": "string"
                            }
                        }
                    },
                    "output_schema": {
                        "type": "object",
                        "properties": {
                            "formatted": {
                                "type": "string"
                            }
                        }
                    }
                }
            ]
        }
    ]
}