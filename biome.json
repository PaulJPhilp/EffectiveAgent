{
  "$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
  "formatter": {
    "enabled": false
  },
  "linter": {
    "enabled": true,
    "rules": {
      "recommended": true,
      "suspicious": {
        "noConsole": "off",
         "noImplicitAnyLet": "error",
         "noExplicitAny": "off"
      },
      "style": {
        "all": false
      },
      "complexity": {
        "all": false
      }
    }
  }
}
