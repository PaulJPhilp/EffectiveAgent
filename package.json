{
  "name": "effective-agent",
  "version": "0.1.0",
  "author": "",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.8",
    "@ai-sdk/google": "^1.2.10",
    "@ai-sdk/groq": "^1.2.7",
    "@ai-sdk/openai": "^1.3.9",
    "@effect/ai": "^0.13.6",
    "@effect/ai-anthropic": "^0.3.6",
    "@effect/ai-openai": "^0.16.7",
    "@effect/platform": "^0.80.6",
    "@effect/platform-bun": "^0.60.11",
    "@effect/platform-node": "^0.76.11",
    "@effect/schema": "^0.75.5",
    "@effect/vitest": "^0.20.6",
    "@langchain/core": "^0.3.43",
    "@langchain/langgraph": "^0.2.63",
    "@langchain/langgraph-sdk": "^0.0.54",
    "ai": "^4.3.4",
    "drizzle-orm": "^0.41.0",
    "effect": "3.14.6",
    "liquidjs": "^10.21.0",
    "pg": "^8.14.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.3",
    "@js-temporal/polyfill": "^0.5.1",
    "@types/bun": "^1.2.8",
    "@types/node": "^20.17.30",
    "@types/pg": "^8.11.11",
    "chalk": "^5.4.1",
    "dotenv": "^16.4.7",
    "drizzle-kit": "^0.30.6",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^1.6.1"
  },
  "bun": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@services/*": ["src/services/*"],
      "@core/*": ["src/services/core/*"],
      "@ai/*": ["src/services/ai/*"],
      "@capabilities/*": ["src/services/capabilities/*"],
      "@execution/*": ["src/services/execution/*"],
      "@memory/*": ["src/services/memory/*"],
      "@agents/*": ["src/agents/*"],
      "@apps/*": ["src/apps/*"]
    }
  },
  "description": "A framework for building sophisticated AI agents using Effect-TS",
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "effect-ts",
    "ai",
    "agent",
    "framework",
    "typescript",
    "bun",
    "langgraph",
    "drizzle"
  ],
  "license": "MIT",
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "dev": "bun --watch src/index.ts",
    "start": "bun dist/index.js",
    "lint": "bunx @biomejs/biome lint ./src",
    "lint:fix": "bunx @biomejs/biome lint --apply ./src",
    "check": "bunx @biomejs/biome check --apply ./src",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "bun src/scripts/migrate.ts",
    "studio": "bunx @langchain/langgraph-cli@latest dev",
    "new:service": "bash ./scripts/create-service.sh"
  },
  "type": "module",
  "types": "dist/index.d.ts"
}
